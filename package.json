{
  "name": "og2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "scan:orphan-params": "node scripts/scan-orphan-params.mjs",
    "scan:magic-numbers": "node scripts/scan-magic-numbers.mjs",
    "db:init": "docker exec -i supabase_db_og2 psql -U postgres -d postgres < drizzle/migrations/init_db.sql",
    "db:generate": "drizzle-kit generate:pg --config ./drizzle.config.ts",
    "db:push": "NEXT_PUBLIC_SUPABASE_POSTGRES_USER=postgres NEXT_PUBLIC_SUPABASE_POSTGRES_PASSWORD=postgres npx drizzle-kit push",
    "test": "vitest run --environment jsdom --setupFiles ./setupTests.ts",
    "test:ui": "vitest --ui --environment jsdom --setupFiles ./setupTests.ts"
  },
  "dependencies": {
    "@ai-sdk/openai-compatible": "^1.0.17",
    "@electric-sql/pglite": "^0.3.8",
    "@hookform/resolvers": "^5.2.1",
    "@supabase/supabase-js": "^2.57.4",
    "ai": "^5.0.44",
    "cheerio": "^1.1.2",
    "dayjs": "^1.11.19",
    "drizzle-orm": "^0.44.5",
    "jotai": "^2.14.0",
    "next": "16.1.2",
    "node-fetch": "^3.3.2",
    "postgres": "^3.4.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "shadcn-ui": "^0.9.5",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "baseline-browser-mapping": "^2.9.14",
    "dotenv-cli": "^10.0.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9",
    "eslint-config-next": "16.1.2",
    "jsdom": "^27.0.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
